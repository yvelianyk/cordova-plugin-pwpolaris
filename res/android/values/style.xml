    </style>
       
    <style name="PanelButtonImage">
        <item name="android:background">@drawable/bg_pressed_selector</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    
     <style name="PanelCheckBoxLeft">
        <item name="android:paddingLeft">0dp</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:button">@null</item>
        <item name="android:drawableLeft">@drawable/cm_checkbox_selector_common</item>
        <item name="android:textColor">@color/panel_sub_title_tcolor</item>
        <item name="android:textSize">@dimen/panel_check_text_size</item>
     </style>
    
     <style name="PanelCheckBoxRight">
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:button">@null</item>
        <item name="android:drawableRight">@drawable/cm_checkbox_selector_common</item>
        <item name="android:textColor">@color/panel_sub_title_tcolor</item>
        <item name="android:textSize">@dimen/panel_check_text_size</item>
    </style>
     
     <style name="PanelSelectImage">
        <item name="android:background">@drawable/bg_pressed_selector</item>
    </style>
    
    <style name="PanelListSheetBlackText">
        <item name="android:paddingLeft">10.00dip</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textColor">@color/panel_list_black_text_color</item>
        <item name="android:textSize">14.00dip</item>
    </style>

