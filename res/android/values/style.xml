    <style name="ChartPanelBlackText">
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">5.33dip</item>
        <item name="android:textSize">16.00sp</item>
        <item name="android:textColor">@color/panel_default_text</item>
        
    </style>
    
    <style name="PenMainBar">
	    <item name="android:background">@drawable/bg_pressed_selector</item>
	    <item name="android:textColor">@drawable/bg_textcolor_selector</item>
	    <item name="android:padding">0dip</item>
	    <item name="android:drawablePadding">-5dip</item>
