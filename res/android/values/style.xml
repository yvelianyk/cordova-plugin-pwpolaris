<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="Theme.DocMaster" parent="android:Theme.DeviceDefault">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/dlg_bg_color</item>
        <item name="android:colorBackground">@color/dlg_bg_color</item>
    </style>

    <style name="NormalTextStyle">
        <item name="android:textColor">@color/normal_text_style_text_color</item>
        <item name="android:textSize">14.67dip</item>
    </style>

    <style name="Title1">
        <item name="android:textColor">@color/title1_text_color</item>
        <item name="android:textSize">20.00dip</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Title2">
        <item name="android:textColor">@color/title2_text_color</item>
        <item name="android:textSize">18.67dip</item>
        <item name="android:textStyle">bold</item>
    </style>    

    <style name="StrongHighlight">
        <item name="android:textColor">@color/strong_highlight_text_color</item>
        <item name="android:textSize">14.67dip</item>
        <item name="android:textStyle">bold|italic</item>
    </style>

    <style name="StrongReference">
        <item name="android:textColor">@color/strong_reference_text_color</item>
        <item name="android:textSize">14.67dip</item>
        <item name="android:textStyle">bold</item>
    </style>

    <!-- //20130818 khchoi To change edit panel gui [start] -->
