        <!-- <item name="android:shadowColor">@color/panel_anchor_title_shdow_color</item>
        <item name="android:shadowDy">2</item>   -->      
    </style>
    
     <style name="PanelButtonTitleText">
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textColor">@color/panel_sub_title_tcolor</item>
        <item name="android:textSize">11sp</item>
    </style>
   
     <style name="PanelButtonTextSize">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/panel_item_tcolor_selector</item>
        <item name="android:textSize">21sp</item>
    </style>
   
     <style name="PanelButtonText">
        <item name="android:background">@drawable/bg_pressed_selector</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textColor">@color/btn_default_tcolor_selector</item>
