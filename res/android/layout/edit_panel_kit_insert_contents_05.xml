        android:background="@drawable/panel_item_bg_selector_common"
        android:clickable="true"
        android:src="@drawable/ico_shape_line_03" />

    <ImageButton
        android:id="@+id/insert04"
        android:layout_width="@dimen/insert_shape_w"
        android:layout_height="match_parent"
        android:layout_marginRight="@dimen/insert_shape_margin_h"
        android:background="@drawable/panel_item_bg_selector_common"
        android:clickable="true"
        android:src="@drawable/ico_shape_line_04" />

    <ImageButton
        android:id="@+id/insert05"
        android:layout_width="@dimen/insert_shape_w"
        android:layout_height="match_parent"
        android:background="@drawable/panel_item_bg_selector_common"
        android:clickable="true"
        android:src="@drawable/ico_shape_line_05" />

</LinearLayout>